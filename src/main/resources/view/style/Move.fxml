        </Button>
        <Button fx:id="btn_delete" layoutX="368.0" layoutY="252.0" mnemonicParsing="false" onAction="#btn_delete" text="DELETE">
            <font>
                <Font name="Chilanka Regular" size="14.0" />
            </font>
        </Button>
        <TableView fx:id="tbl_movie" layoutX="56.0" layoutY="302.0" prefHeight="200.0" prefWidth="521.0">
            <columns>
                <TableColumn prefWidth="251.0" text="ID" />
                <TableColumn prefWidth="265.0" text="NAME" />
            </columns>
        </TableView>
      <Button fx:id="btn_dashboard" layoutX="7.0" layoutY="3.0" mnemonicParsing="false" onAction="#btn_dashboard" prefHeight="26.0" prefWidth="100.0" text="DasheBoard" />
