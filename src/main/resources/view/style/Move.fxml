<AnchorPane prefHeight="564.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.sCs.moveManagement.controller.MoveController">
    <children>
        <Label fx:id="lbl_managemove" alignment="CENTER" layoutX="159.0" layoutY="29.0" prefHeight="37.0" prefWidth="297.0" text="Manage Movies">
            <font>
                <Font name="Chilanka Regular" size="33.0" />
            </font>
        </Label>
        <Label layoutX="77.0" layoutY="117.0" prefHeight="41.0" prefWidth="107.0" text="Movie ID">
            <font>
                <Font name="Chilanka Regular" size="21.0" />
            </font>
        </Label>
        <TextField fx:id="txt_movieid" layoutX="239.0" layoutY="112.0" prefHeight="41.0" prefWidth="271.0" promptText="Movie ID">
         <effect>
            <Glow />
         </effect>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></TextField>
        <Label layoutX="77.0" layoutY="180.0" prefHeight="41.0" prefWidth="122.0" text="Movie Name">
            <font>
                <Font name="Chilanka Regular" size="21.0" />
